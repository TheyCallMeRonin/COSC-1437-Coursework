public class Factorials
{
   public static void main (String args[])
   {
      // Write your code here
    int i;
    int range = 10;
    int fact = 1;
    int j;

    for (i = 1; i <= range; i++)
    {
      for (j = 1; j <= i; j++)
      {
        fact = fact * j;
      }
      System.out.println("The factorial of " + i + " is " + fact);
      fact = 1;
      j = 1;
    }

      
      
   }
}
